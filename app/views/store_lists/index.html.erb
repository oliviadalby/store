<p id="notice"><%= notice %></p>

<h1>Store Lists</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th>Open</th>
      <th>Employees</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @store_lists.each do |store_list| %>
      <tr>
        <td><%= store_list.name %></td>
        <td><%= store_list.open %></td>
        <td><%= store_list.employees %></td>
        <td><%= link_to 'Show', store_list %></td>
        <td><%= link_to 'Edit', edit_store_list_path(store_list) %></td>
        <td><%= link_to 'Destroy', store_list, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Store List', new_store_list_path %>
